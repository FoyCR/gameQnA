    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>603f4236-7a2c-40ac-a872-fa8bbe5ac082</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
